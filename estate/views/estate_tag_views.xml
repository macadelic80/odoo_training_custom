<?xml version="1.0"?>
<odoo>
  <record id="estate_tag_views" model="ir.actions.act_window">
    <field name="name">Properties Tags</field>
    <field name="res_model">estate.property.tag</field>
    <field name="view_mode">tree,form</field>
  </record>

  <record id="estate_tag_tree" model="ir.ui.view">
    <field name="name">estate.property.tag.view.tree</field>
    <field name="model">estate.property.tag</field>
    <field name="arch" type="xml">
      <tree string="Channel">
        <field name="name"/>
      </tree>
    </field>
  </record>
</odoo>